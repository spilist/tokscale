{
  "name": "@0xinevitable/token-tracker",
  "version": "1.0.2",
  "description": "Calculate token prices from OpenCode, Claude Code, Codex, Gemini, and Cursor sessions",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cli.js",
  "bin": {
    "token-tracker": "./dist/cli.js"
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xinevitable/token-usage-tracker.git",
    "directory": "packages/cli"
  },
  "scripts": {
    "build": "tsc",
    "dev": "bun src/cli.ts",
    "dev:node": "tsx src/cli.ts",
    "tui": "bun src/cli.ts tui",
    "start": "node dist/cli.js",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@0xinevitable/token-tracker-core": "^0.1.1",
    "@opentui/core": "0.1.60",
    "@opentui/react": "^0.1.60",
    "@opentui/solid": "^0.1.60",
    "cli-table3": "^0.6.5",
    "clipboardy": "^5.0.2",
    "commander": "^14.0.2",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "picocolors": "^1.1.1",
    "react": "^19.2.3",
    "solid-js": "1.9.9",
    "string-width": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.7",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "keywords": [
    "token-tracker",
    "opencode",
    "claude-code",
    "cursor",
    "gemini",
    "codex",
    "cli"
  ]
}
